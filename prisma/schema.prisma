generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Restaurnat {
  id Int @id @default(autoincrement())
  name String
  station String
  prefecture String
  tweetText String
  tweetId String @unique
  imageUrl String?
  imageUrlInTweetCard String?
  dateTime DateTime?
  userName String
  userId String
}

model Book {
  id Int @id @default(autoincrement())
  name String
  title String?
  text String?
  genre String?
}
